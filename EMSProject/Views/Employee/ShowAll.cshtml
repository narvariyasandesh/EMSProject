@Model EMSProject.Models;

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>  
      <th scope="col">Image</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>

    </tr>
  </thead>
  <tbody>
@foreach (var item in Model)
{
    <tr>
      <th scope="row">@item.Id</th>
      <td><img src="@item.Image" height="60px" width="80px"/> </td>
      <td>@item.FirstName &ensp; @item.LastName</td>
      <td>@item.Email</td>
      <td>@item.Mobile</td>
      <td><a href="#">Edit</a></td>
      <td><a asp-action="Delete" asp-controller="Employee">Delete</a></td>
    </tr>
}
   
  </tbody>
</table>